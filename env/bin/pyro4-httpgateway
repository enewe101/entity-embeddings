#!/home/ndg/users/enewel3/entity-embeddings/env/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'Pyro4','console_scripts','pyro4-httpgateway'
__requires__ = 'Pyro4'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('Pyro4', 'console_scripts', 'pyro4-httpgateway')()
    )
